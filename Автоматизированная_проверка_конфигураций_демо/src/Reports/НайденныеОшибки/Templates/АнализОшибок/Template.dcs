			И НЕ НайденныеОшибки.Ответственный.Ссылка ЕСТЬ NULL
			ТОГДА НайденныеОшибки.Объект
		КОГДА НайденныеОшибки.Объект.ТипОбъекта В (
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.Форма),
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.Макет),
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.ТабличнаяЧасть),
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.Графа),
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.Измерение),
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.Ресурс),
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.Значение),
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.УправляемаяФорма),
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.СтандартныйРеквизит),
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.Команда),
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.ПредопределенныйЭлемент),
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.Операция),
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.ШаблонURL)
			)
			ТОГДА НайденныеОшибки.Объект.Родитель
		КОГДА НайденныеОшибки.Объект.ТипОбъекта = ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.Реквизит)
			И НайденныеОшибки.Объект.Родитель.ТипОбъекта = ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.ТабличнаяЧасть)
			ТОГДА НайденныеОшибки.Объект.Родитель.Родитель
		КОГДА НайденныеОшибки.Объект.ТипОбъекта = ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.Реквизит)
			И НайденныеОшибки.Объект.Родитель.ТипОбъекта &lt;&gt; ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.ТабличнаяЧасть)
			ТОГДА НайденныеОшибки.Объект.Родитель
		КОГДА НайденныеОшибки.Объект.ТипОбъекта = ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.Подсистема)
			ТОГДА ВЫБОР
				КОГДА НайденныеОшибки.Объект.Родитель.Ответственный.Ссылка ЕСТЬ NULL
					ТОГДА НайденныеОшибки.Объект.Родитель.Родитель
				ИНАЧЕ
					НайденныеОшибки.Объект.Родитель
			КОНЕЦ
		КОГДА НайденныеОшибки.Объект.ТипОбъекта В (
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.Параметр),
			ЗНАЧЕНИЕ(Перечисление.ТипыОбъектов.МетодШаблонаURL)
			)
			ТОГДА НайденныеОшибки.Объект.Родитель.Родитель
	КОНЕЦ КАК Родитель

ПОМЕСТИТЬ РодителиИОтветственныеОбъекта
ИЗ
	РегистрСведений.НайденныеОшибки КАК НайденныеОшибки
	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.СтруктураКонфигурации КАК СтруктураКонфигурации
		ПО НайденныеОшибки.Объект = СтруктураКонфигурации.Ссылка
	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ОбнаруживаемыеОшибки КАК ОбнаруживаемыеОшибки
		ПО НайденныеОшибки.Ошибка = ОбнаруживаемыеОшибки.Ссылка
ГДЕ
	СтруктураКонфигурации.Владелец = &amp;Версия
	И НайденныеОшибки.Состояние &lt;&gt; &amp;СостояниеОсобенность
	И ((ОбнаруживаемыеОшибки.Критичность = ЗНАЧЕНИЕ(Перечисление.УровниКритичностиОшибок.ПустаяСсылка))
		ИЛИ ОбнаруживаемыеОшибки.Критичность.Порядок &lt;= &amp;МинимальнаяКритичность)
	И (&amp;НеОтбиратьПоОшибке
		ИЛИ НайденныеОшибки.Ошибка = &amp;Ошибка)
	И (&amp;НеОтбиратьПоСостоянию
		ИЛИ НайденныеОшибки.Состояние = &amp;Состояние)
;

ВЫБРАТЬ
	НайденныеОшибки.НОМЕР,
	НайденныеОшибки.Объект,
	ВЫБОР
		КОГДА НЕ ТабличнаяЧастьОбъектаПодсистема.Подсистема.Ответственный.Ссылка ЕСТЬ NULL
