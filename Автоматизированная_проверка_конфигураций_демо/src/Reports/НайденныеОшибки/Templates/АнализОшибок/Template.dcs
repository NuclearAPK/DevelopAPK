			ИЛИ НЕ ТабличнаяЧастьОбъектаПодсистема.Подсистема.Родитель.Ответственный.Ссылка ЕСТЬ NULL
			ИЛИ НЕ ТабличнаяЧастьОбъектаПодсистема.Подсистема.Родитель.Родитель.Ответственный.Ссылка ЕСТЬ NULL)
ГДЕ
	НайденныеОшибки.Ответственный.Ссылка ЕСТЬ NULL
;

ВЫБРАТЬ
	НайденныеОшибки.НОМЕР,
