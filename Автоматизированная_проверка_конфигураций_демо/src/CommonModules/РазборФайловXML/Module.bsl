	СоответствиеЗначений.Вставить("AutoUpdate", "ОбновлятьАвтоматически");
	
	// Имена атрибутов регламентного задания.
	СоответствиеЗначений.Вставить("BeginDate", "ДатаНачала");
	СоответствиеЗначений.Вставить("EndDate", "ДатаКонца");
